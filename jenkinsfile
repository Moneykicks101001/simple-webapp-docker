pipeline {
    agent any
        stage('Append Route') {
            steps {
                script {
                    // Execute the shell script to append the route
                    sh './append.sh'
                }
            }
        }
        stage('Show File') {
            steps {
                script {
                    // Execute the shell script to append the route
                    sh 'cat app.py'
                }
            }
        }
}
